	<queryString><![CDATA[SELECT stranke.kol_os as stranke_kom,
	stranke.cena_naj as stranke_najem,
	stranke.kol_os * stranke.cena_naj as stranke_vrednost,
CONCAT( stranke.`naziv` , ' ', stranke.`naslov` , ' ', stranke.`posta` , ' ', stranke.`kraj` ) AS stranke_naziv,
osnovna.osnovna as osnovna
FROM (SELECT stranke.*
	FROM stranke, (SELECT sif_str, max(veljavnost) datum 
			FROM stranke
			where veljavnost <= CAST($P{do_datum} AS DATE)
		 	group by sif_str ) zadnji
	WHERE stranke.sif_str = zadnji.sif_str and
	      stranke.veljavnost = zadnji.datum) stranke,
(SELECT osnovna.*
	FROM osnovna, (SELECT sif_os, max(zacetek) datum FROM osnovna
 	group by sif_os ) zadnji
	WHERE osnovna.sif_os = zadnji.sif_os and
	      osnovna.zacetek = zadnji.datum ) osnovna
WHERE stranke.sif_kupca = $P{sif_str} and
      stranke.najem = "X" and
      stranke.sif_os = osnovna.sif_os and
      NOT EXISTS (select sif_kupca 
			from $P!{dobLeto} dob 
			where dob.sif_str = stranke.sif_str and 
				datum >= CAST($P{od_datum} AS DATE) 
				AND datum <= CAST($P{do_datum} AS DATE))
ORDER BY stranke_naziv]]></queryString>

