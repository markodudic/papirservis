	<queryString><![CDATA[SELECT kupci.`sif_kupca` AS sif_kupca,
     kupci.`naziv` AS kupci_naziv,
     kupci.`naslov` AS kupci_naslov,
     kupci.`posta` AS kupci_posta,
     kupci.`kraj` AS kupci_kraj,
     uporabniki.ime_in_priimek as potnik,
     skup.tekst as skupina,
     kupci.`sif_rac` AS kupci_sif_rac,
     kupci.`opomba` AS kupci_opomba
FROM kupci, uporabniki, skup, enote,
(
SELECT kupci.sif_kupca, IFNULL(a.acena, 0) + IFNULL(b.najem, 0) + IFNULL(c.neodvoz, 0) 
FROM kupci left join
(SELECT IFNULL(SUM(dob.`cena`), 0) + IFNULL(SUM(dob.`kg_zaup` * dob.`sit_zaup`), 0) + IFNULL(SUM(dob.`kolicina` * dob.`sit_smet`), 0) AS acena,
dob.sif_kupca, dob.sif_enote
FROM $P!{dobLeto} dob, (SELECT st_dob, pozicija, max(zacetek) datum 
		FROM $P!{dobLeto} dob 
		WHERE obdelana > 0
     			AND dob.datum >= CAST($P{od_datum} AS DATE) 
     			AND dob.datum <= CAST($P{do_datum} AS DATE)
		group by st_dob, pozicija) zadnji
WHERE dob.st_dob = zadnji.st_dob and
      dob.pozicija = zadnji.pozicija and
      dob.zacetek = zadnji.datum 
group by sif_kupca) as a on (kupci.sif_kupca = a.sif_kupca)
left join
(SELECT SUM(stranke.kol_os * stranke.cena_naj) as najem,
stranke.sif_kupca 
FROM (SELECT stranke.*
	FROM stranke, (SELECT sif_str, max(zacetek) datum FROM stranke
 	group by sif_str ) zadnji
	WHERE stranke.sif_str = zadnji.sif_str and
	      stranke.zacetek = zadnji.datum) stranke
WHERE ((stranke.najem = "D") 
	or 
     	(stranke.sif_kupca NOT IN (select sif_kupca 
			from $P!{dobLeto} dob 
			where datum >= CAST($P{od_datum} AS DATE) 
			AND datum <= CAST($P{do_datum} AS DATE))))
group by stranke.sif_kupca ) as b on (kupci.sif_kupca = b.sif_kupca)
left join
(SELECT SUM(stranke.kol_os * stranke.cena_naj) as neodvoz,
stranke.sif_kupca 
FROM (SELECT stranke.*
	FROM stranke, (SELECT sif_str, max(zacetek) datum FROM stranke
 	group by sif_str ) zadnji
	WHERE stranke.sif_str = zadnji.sif_str and
	      stranke.zacetek = zadnji.datum) stranke
WHERE stranke.najem = "X" and
      NOT EXISTS (select sif_kupca 
			from $P!{dobLeto} dob 
			where dob.sif_str = stranke.sif_str and 
				datum >= CAST($P{od_datum} AS DATE) 
				AND datum <= CAST($P{do_datum} AS DATE))
group by stranke.sif_kupca) as c on (kupci.sif_kupca = c.sif_kupca)
where IFNULL(a.acena, 0) + IFNULL(b.najem, 0) + IFNULL(c.neodvoz, 0) > 0
) as imajoceno
WHERE ((kupci.sif_kupca = $P{sif_str}) or (($P{sif_str} = -1))) and 
     kupci.skupina = skup.skupina and
     ((enote.sif_enote = $P{sif_enote}) or ($P{sif_enote} = -1)) and
     exists (select * 
			from $P!{dobLeto} dob 
			where kupci.sif_kupca = dob.sif_kupca and kupci.sif_enote = enote.sif_enote and
			datum >= CAST($P{od_datum} AS DATE) 
			AND datum <= CAST($P{do_datum} AS DATE)) and
     ((skup.skupina = $P{sif_skupine}) or ($P{sif_skupine} = -1)) and
     kupci.potnik = uporabniki.sif_upor and
    kupci.sif_kupca = imajoceno.sif_kupca]]></queryString>

