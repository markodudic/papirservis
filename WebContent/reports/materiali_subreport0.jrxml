	<queryString><![CDATA[SELECT 
dob.`koda` AS dob_koda, 
SUM(dob.`kolicina`) AS dob_kolicina, 
SUM(dob.`kolicina` * cenastr.`cena`) AS dob_vrednost, 
materiali.`material` AS material
FROM 
     (SELECT materiali.*
	FROM materiali, (SELECT koda, max(zacetek) datum FROM materiali group by koda) zadnji
	WHERE materiali.koda = zadnji.koda and
	      materiali.zacetek = zadnji.datum) materiali, 
     (SELECT dob.*
	FROM $P!{dobLeto} dob, (SELECT st_dob, pozicija, max(zacetek) datum 
		FROM $P!{dobLeto} dob 
		WHERE dob.datum >= CAST($P{od_datum} AS DATE) 
			AND dob.datum <= CAST($P{do_datum} AS DATE) 
		group by st_dob, pozicija) zadnji
	WHERE dob.st_dob = zadnji.st_dob and
	      dob.pozicija = zadnji.pozicija and
	      dob.zacetek = zadnji.datum)  dob
LEFT JOIN
     (SELECT cenastr.*
	FROM cenastr, (SELECT id, sif_kupca, material_koda, max(zacetek) datum 
			FROM cenastr 
			WHERE cenastr.veljavnost <= CAST($P{do_datum} AS DATE)
			group by sif_kupca, material_koda) zadnji
	WHERE cenastr.material_koda = zadnji.material_koda and 
		cenastr.zacetek = zadnji.datum) cenastr 
ON (dob.koda = cenastr.material_koda and dob.sif_kupca = cenastr.sif_kupca),
     enote
WHERE dob.`koda` = materiali.`koda` and
	dob.sif_enote = enote.sif_enote and
	((enote.nadenota = $P{nadenota}) OR ($P{nadenota} = "Vse nadenote")) and
	((dob.sif_enote = $P{sif_enote}) OR ($P{sif_enote} = -1)) and
	((dob.skupina = $P{sif_skupine}) OR ($P{sif_skupine} = -1))
GROUP BY dob.`koda`, materiali.`material`
ORDER BY dob.`koda`]]></queryString>

