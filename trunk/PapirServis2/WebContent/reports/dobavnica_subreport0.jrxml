	<queryString><![CDATA[SELECT distinct
     dob.`koda` AS koda,
     dob.`ewc` AS ewc,
     materiali.material as material
FROM
     (select *, max(dob.zacetek) from $P!{dobLeto}  dob where  dob.st_dob =  $P{st_dob}  group by st_dob, pozicija) dob LEFT JOIN sofer ON (dob.`sif_sof` = sofer.`sif_sof`),
	(SELECT materiali.*
	FROM materiali, (SELECT koda, max(zacetek) datum FROM materiali group by koda) zadnji
	WHERE materiali.koda = zadnji.koda and
	      materiali.zacetek = zadnji.datum) materiali	     
WHERE dob.koda = materiali.koda]]></queryString>

